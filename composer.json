{
    "name": "moriony/rpc-server",
    "description": "JSON+XML RPC Server based on Symfony components.",
    "type": "library",
    "keywords": ["rpc", "xml", "json", "server", "symfony"],
    "license": "MIT",
    "authors": [
        {
            "name": "Vitaliy Chesnokov",
            "email": "demateus@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/event-dispatcher": "~2.4",
        "symfony/http-foundation": "~2.4",
        "symfony/config": "~2.4",
        "symfony/dependency-injection": "~2.4",
        "pagerfanta/pagerfanta": "~1.0"
    },
    "suggest": {
        "jms/serializer": "^1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "satooshi/php-coveralls": "0.6.*"
    },
    "autoload": {
        "psr-4": {
            "Moriony\\RpcServer\\": "src/"
        }
    }
}
